import "../../../interfaces/ChanMuxInf.camkes";
import "../../../interfaces/UartInf.camkes";


component ChanMux {

    provides       ChanMuxNwStackInf        ChanMuxNwStack;
    //-------------------------------------------------
    // Network Stack interface
    dataport       Buf                      nwStackDataPortRead;
    dataport       Buf                      nwStackDataPortWrite;
    dataport       Buf                      nwStackCtrlDataPort;
    emits          NwStack_DataAvailable    e_read_nwstacktick;


    //-------------------------------------------------
    // Network Stack 2 interface
    dataport       Buf                      nwStackDataPortRead_2;
    dataport       Buf                      nwStackDataPortWrite_2;
    dataport       Buf                      nwStackCtrlDataPort_2;
    emits          NwStack_DataAvailable    e_read_nwstacktick_2;




    //-------------------------------------------------
    // MUX Uart interface
    dataport      Buf                       outputDataPort;
    uses          UartInf                   Output;
    provides      ChanMuxOutInf             ChanMuxOut;

}
